Journal = {IEEE/ACM Transactions on Computational Biology and Bioinformatics, Computational Biology and Bioinformatics, IEEE/ACM Transactions on, IEEE/ACM Trans. Comput. Biol. and Bioinf.},
Keywords = {Bioengineering, Computing and Processing, Memristors, Integrated circuit modeling, Image processing, Fatigue, Threshold voltage, Software algorithms, Computer architecture, Memristive circuit design, image binarization, eyes state detection, biological long short-term memory (B-LSTM)},
